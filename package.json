{
  "name": "hegel-rost",
  "version": "1.0.0",
  "description": "Web application to access IP-control features of the Hegel Röst",
  "keywords": [
    "Hegel",
    "Röst",
    "IP-control"
  ],
  "main": "app.js",
  "author": "Joost Berns",
  "license": "MIT",
  "scripts": {
    "dev-api": "DEBUG=hr:* nodemon -w src -w .env -w api.js --exec babel-node api.js",
    "dev-app": "DEBUG=hr:* webpack-dev-server --config webpack.config.js",
    "dev-device": "DEBUG=hr:* nodemon -w device.js --exec babel-node device.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start-api": "forever start --uid 'hegel-rost-api' -a -c babel-node api.js",
    "stop-api": "forever stop hegel-rost-api",
    "restart-api": "forever restart hegel-rost-api",
    "start-app": "forever start --uid 'hegel-rost-app' -a -c babel-node app.js",
    "stop-app": "forever stop hegel-rost-app",
    "restart-app": "forever restart hegel-rost-app",
    "start": "npm run start-api && npm run start-app",
    "stop": "npm run stop-api && npm run stop-app",
    "restart": "npm run restart-api && npm run restart-app"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^5.0.2",
    "cors": "^2.8.5",
    "css-loader": "^2.1.1",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-queue": "^0.0.12",
    "file-loader": "^3.0.1",
    "forever": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vue-template-loader": "^1.0.0",
    "vuetify": "^1.5.11",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandalor/hegel-rost.git"
  }
}
